<div class="col-xs-12">
  <div class="flex-container">
    <div class="flex-block">

    <h3>Results</h3>
      <% if @photoArray.any? %>
        <% @photoArray.each do |picture| %>
          <div class="col-xs-6">
            <div class="well well-small">
              <%= link_to image_tag(picture.image, :class => "img-responsive round"), picture_path(picture) %>
              <div id="block1-mini">
                <span class="month"><%= picture.created_at.strftime("%b") %></span>
                <span class="day"><%= picture.created_at.strftime("%d") %></span>
                <div class="title"><%= picture.title %></div>
                <%= link_to image_tag("btn-fav.png", :class => 'fav-button'), {:fav => picture.id} %>
                <%= link_to image_tag("btn-edit.png", :class => "btn edit-button"), picture_path(picture) %>
              </div>
              <div id="block-bottom">
                <span class="time"><%# time_ago_in_words(picture.created_at) %></span>
              </div>
            </div><!-- well end -->
          </div><!-- col end -->
        <% end %>
        <% else %>
          <p>There are no outfits with this name.</p>
        <% end %>

    </div><!-- flex-block end -->
  </div><!-- flex-container end -->
</div><!-- col end -->
