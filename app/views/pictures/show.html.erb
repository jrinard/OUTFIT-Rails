

<div class="col-sm-12 col-md-6">

  <%= render "form-details" %>
  <%= link_to "Delete", picture_path(@picture),:data => {:confirm => "Are you sure?",:method => "delete"}, remote: true  %>

<div class="well">
  <div id="details">
    <% if @picture.body != "" %>
      <span class="type">Body:</span> <span class="detail"><%= @picture.body %></span><br>
    <% end %>
    <% if @picture.legs != ""  %>
      <span class="type">Legs:</span> <span class="detail"> <%= @picture.legs %></span><br>
    <% end %>
    <% if @picture.feet != "" %>
      <span class="type">Feet:</span> <span class="detail"> <%= @picture.feet %></span><br>
    <% end %>
    <% if @picture.hands != "" %>
      <span class="type">Hands:</span> <span class="detail"> <%= @picture.hands %></span><br>
    <% end %>
    <% if @picture.head != "" %>
      <span class="type">Head:</span> <span class="detail"> <%= @picture.head %></span><br>
    <% end %>
    <% if @picture.extra != "" %>
      <span class="type">Extra:</span> <span class="detail"> <%= @picture.extra %></span><br>
    <% end %>
  </div><!-- detail end -->
</div><!-- well end -->


</div> <!-- col end -->
<hr>


Title: <%= @picture.title %><br>
Task Id: <%= @task_id %>


